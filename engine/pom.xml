    <build>
        <finalName>testlum</finalName>

        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>jaxb2-maven-plugin</artifactId>
                <version>${jaxb2-maven-plugin.version}</version>
                <configuration>
                    <outputDirectory>${models.dir}</outputDirectory>
                    <clearOutputDir>true</clearOutputDir>
                    <sources>
                        <source>${schema.path}${file.separator}scenario.xsd</source>
                        <source>${schema.path}${file.separator}global-config.xsd</source>
                        <source>${schema.path}${file.separator}pages.xsd</source>
                    </sources>
                    <noGeneratedHeaderComments>true</noGeneratedHeaderComments>
                </configuration>
            </plugin>

            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-assembly-plugin</artifactId>
                <version>${maven-assembly-plugin.version}</version>
                <configuration>
                    <descriptors>
                        <descriptor>src/main/resources/assembly-plugin-configuration.xml</descriptor>
                    </descriptors>
                </configuration>
                <executions>
                    <execution>
                        <id>make-assembly</id>
                        <phase>package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <archive>
                                <manifest>
                                    <mainClass>${start-class}</mainClass>
                                </manifest>
                            </archive>
                        </configuration>
                    </execution>
                </executions>
            </plugin>

        </plugins>
    </build>
