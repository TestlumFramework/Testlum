    <Match>
        <Bug pattern="RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE"/>
    </Match>

    <Match>
        <Class name="com.knubisoft.ui.testing.framework.util.FileRemover$FileVisitor"/>
        <Method name="visitFile"/>
