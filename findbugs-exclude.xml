    </Match>
    <Match>
        <Class name="com.knubisoft.cott.testing.framework.scenario.ScenarioValidator"/>
        <Bug pattern="UC_USELESS_OBJECT"/>
    </Match>
    <Match>
        <Class name="com.knubisoft.ui.testing.framework.interpreter.AuthInterpreter"/>
        <Bug pattern="VA_FORMAT_STRING_USES_NEWLINE"/>
    </Match>
    <Match>
        <Class name="com.knubisoft.ui.testing.framework.interpreter.VariableInterpreter"/>
