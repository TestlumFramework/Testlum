    </Match>

    <Match>
        <Class name="com.knubisoft.ui.testing.framework.parser.XMLParser"/>
        <Bug pattern="DLS_DEAD_LOCAL_STORE"/>
    </Match>
    <Match>
        <Class name="com.knubisoft.ui.testing.framework.scenario.ScenarioValidator"/>
        <Bug pattern="URF_UNREAD_FIELD"/>
    </Match>

    <Match>
        <Class name="com.knubisoft.ui.testing.framework.scenario.ScenarioRunner"/>
        <Bug pattern="REC_CATCH_EXCEPTION"/>
    </Match>

    <Match>
        <Class name="com.knubisoft.ui.testing.framework.util.FileSearcher"/>
        <Bug pattern="DMI_HARDCODED_ABSOLUTE_FILENAME"/>
    </Match>

    <Match>
        <Class name="com.knubisoft.cott.testing.framework.util.ArgumentsUtils"/>
