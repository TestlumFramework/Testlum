<page xmlns="http://www.knubisoft.com/e2e/testing/model/pages">

    <details>
        <name>The Add profile photo page</name>
        <url>https://pme-dev-en.xyz/register/user-photo</url>
        <description>Page for uploading photo which will be used for owner logo</description>
    </details>

    <locators>

        <locator locatorId="userProfileTitle">
            <class>register-info-title main-title</class>
        </locator>

        <locator locatorId="selectPhotoButton">
            <class>input__file-button</class>
        </locator>

        <locator locatorId="addProfilePhotoButton">
            <id>register_profile-photo_input</id>
        </locator>

        <locator locatorId="confirmButton">
            <id>register_profile-photo_button</id>
        </locator>

        <locator locatorId="negativePhotoScenario">
            <id>register_profile-photo_error</id>
        </locator>

    </locators>
</page>
