<scenario xmlns="http://www.knubisoft.com/e2e/testing/model/scenario">

    <overview>
        <description>Self s3 test</description>
        <name>S3 test scenario</name>
    </overview>

    <tags>
        <tag>self_test</tag>
    </tags>

    <s3 comment="Compare file from empty bucket with expected file"
        alias="bucketone"
        key="/com/knubisoft/integration-testing/expected_1.json">
        <download>expected_1.json</download>
    </s3>

    <s3 comment="Upload json file to the bucket"
        alias="bucketone"
        key="/com/knubisoft/integration-testing/expected_2.json">
        <upload>expected_2.json</upload>
    </s3>

    <s3 comment="Compare json file from bucket with expected file"
        alias="bucketone"
        key="/com/knubisoft/integration-testing/expected_2.json">
        <download>expected_2.json</download>
    </s3>

    <s3 comment="Upload jpeg file to the bucket"
        alias="bucketone"
        key="/com/knubisoft/integration-testing/expected_4.jpeg">
        <upload>expected_4.jpeg</upload>
    </s3>

    <s3 comment="Compare jpeg file from bucket with expected file"
        alias="bucketone"
        key="/com/knubisoft/integration-testing/expected_4.jpeg">
        <download>expected_4.jpeg</download>
    </s3>

    <s3 comment="Upload .pdf file to the bucket"
        alias="bucketone"
        key="/com/knubisoft/integration-testing/expected_6.pdf">
        <upload>expected_6.pdf</upload>
    </s3>

    <s3 comment="Compare pdf file from bucket with expected file"
        alias="bucketone"
        key="/com/knubisoft/integration-testing/expected_6.pdf">
        <download>expected_6.pdf</download>
    </s3>

    <s3 comment="Test downloading pdf file from previous expected again"
        alias="bucketone"
        key="/com/knubisoft/integration-testing/expected_6.pdf">
        <download>expected_6.pdf</download>
    </s3>

    <s3 comment="Fill 'expected_1.json' key with expected_2.json file to test clearing before next scenario"
        alias="bucketone"
        key="/com/knubisoft/integration-testing/expected_1.json">
        <upload>expected_2.json</upload>
    </s3>
<!--BUCKET TWO-->
    <s3 comment="Compare file from empty bucket with expected file"
        alias="buckettwo"
        key="/com/knubisoft/integration-testing/expected_1.json">
        <download>expected_1.json</download>
    </s3>

    <s3 comment="Upload json file to the bucket"
        alias="buckettwo"
        key="/com/knubisoft/integration-testing/expected_2.json">
        <upload>expected_2.json</upload>
    </s3>

    <s3 comment="Compare json file from bucket with expected file"
        alias="buckettwo"
        key="/com/knubisoft/integration-testing/expected_2.json">
        <download>expected_2.json</download>
    </s3>

    <s3 comment="Upload jpeg file to the bucket"
        alias="buckettwo"
        key="/com/knubisoft/integration-testing/expected_4.jpeg">
        <upload>expected_4.jpeg</upload>
    </s3>

    <s3 comment="Compare jpeg file from bucket with expected file"
        alias="buckettwo"
        key="/com/knubisoft/integration-testing/expected_4.jpeg">
        <download>expected_4.jpeg</download>
    </s3>

    <s3 comment="Upload .pdf file to the bucket"
        alias="buckettwo"
        key="/com/knubisoft/integration-testing/expected_6.pdf">
        <upload>expected_6.pdf</upload>
    </s3>

    <s3 comment="Compare pdf file from bucket with expected file"
        alias="buckettwo"
        key="/com/knubisoft/integration-testing/expected_6.pdf">
        <download>expected_6.pdf</download>
    </s3>

    <s3 comment="Test downloading pdf file from previous expected again"
        alias="buckettwo"
        key="/com/knubisoft/integration-testing/expected_6.pdf">
        <download>expected_6.pdf</download>
    </s3>

    <s3 comment="Fill 'expected_1.json' key with expected_2.json file to test clearing before next scenario"
        alias="buckettwo"
        key="/com/knubisoft/integration-testing/expected_1.json">
        <upload>expected_2.json</upload>
    </s3>

</scenario>
