<?xml version="1.0" encoding="UTF-8"?>
<x:schema xmlns:x="http://www.w3.org/2001/XMLSchema"
          targetNamespace="http://www.knubisoft.com/cott/testing/model/global-config"
          xmlns:tns="http://www.knubisoft.com/cott/testing/model/global-config"
          elementFormDefault="qualified"
          xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" jaxb:version="2.1">

    <x:include schemaLocation="shared-config.xsd"/>

    <x:complexType name="websockets">
        <x:choice>
            <x:element name="api" type="tns:websocketApi" maxOccurs="unbounded"/>
        </x:choice>
    </x:complexType>

    <x:complexType name="websocketApi">
        <x:complexContent>
            <x:extension base="tns:integration">
                <x:attribute name="url" type="tns:urlWebsocket" use="required"/>
                <x:attribute name="stomp" type="x:boolean" default="false"/>
            </x:extension>
        </x:complexContent>
    </x:complexType>

</x:schema>
